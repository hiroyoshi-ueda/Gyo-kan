<div class="admin top container">
<h2>行持カレンダー</h2>
  <div class="row">
  	<div class='col-xs-6'>
  	  <h4>　　本日の行持予定</h4>
  	<table class="table table-event-today">
  	  <% @events. each do |event| %>
  	    <tr><tr>
  	    <tr></tr>
  	    <th></th>
  	    <td>
  	    	<%= link_to admins_event_path(event.id) do %>
  	    	<%= event.title %>
  	    	<% end %>
  	    </td>
  	  <% end %>
  	</table>
  	</div>

  	<div class='col-xs-6'>
  	  <div id="calendar"></div>
  	</div>
  </div>
</div>
